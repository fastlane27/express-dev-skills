<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dev Skills</title>
</head>
<body>
    <h1>Skills</h1>
    <ul>
        <% skills.forEach(function(skill, idx) { %>
            <li>
                <a href="/skills/<%= idx %>"><%= skill.skill %></a>
            </li>
        <% }); %>
    </ul>
    <a href="/skills/new">Add New Skill</a>
</body>
</html>
